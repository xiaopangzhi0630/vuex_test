/* 主题1 */
.theme1 .aside{
    background: url(../../../static/bg_aside1.png) center/cover no-repeat;
}
.theme1 .aside .menus .item .item-content .active {
    color: #051962;
}
.theme1 .top{
    background: url(../../../static/bg_top1.png) center/100% 100% no-repeat;
}
.theme1 .top .homePage{
    color: #fff;
}
.theme1 .top .fastLinks a{
    color: #fff;
}
.theme1 .top .fastLinks a:hover{
    color: #1347b3;
    background-color: #fff;
}
.theme1 .top .fastLinks .active{
    color: #1347b3;
    background-color: #fff;
}
.theme1 .switches{
    color: #fff;
}
.theme1 .systemTool .toolItem{
    color: #fff;
}
.theme1 .systemTool .toolItem .symbol{
    color: #fff;
}
.theme1 .el-dropdown-link{
    color: #fff;
}
.theme1 .el-input.is-active .el-input__inner, .theme1 .el-input__inner:focus {
    border-color: #3A6073;
}
.theme1 .el-button--primary{
    background:#1245b1;
    border-color:#1245b1;
}
.theme1 .el-button--primary:hover{
    background:rgba(18,69,177,0.8);
    border-color:rgba(18,69,177,0.8);
}

