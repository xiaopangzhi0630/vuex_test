/* 主题3 */
.theme3 .aside{
    background: #16222A;
    background: linear-gradient(to bottom, #16222A, #3A6073);
}
.theme3 .aside .menus .item .item-content .active {
    color: #16222A;
}
.theme3 .top{
    background: #3A6073;
}
.theme3 .top .homePage{
    color: #fff;
}
.theme3 .top .fastLinks a{
    color: #fff;
}
.theme3 .top .fastLinks a:hover{
    color: #3a6073;
    background-color: #fff;
}
.theme3 .top .fastLinks .active{
    color: #3a6073;
    background-color: #fff;
}
.theme3 .switches{
    color: #fff;
}
.theme3 .systemTool .toolItem{
    color: #fff;
}
.theme3 .systemTool .toolItem .symbol{
    color: #fff;
}
.theme3 .el-dropdown-link{
    color: #fff;
}
.theme3 .el-radio__inner:hover {
    border-color: #3A6073;
}
.theme3 .el-radio__input.is-checked .el-radio__inner {
    border-color: #3A6073;
    background: #3A6073;
}
.theme3 .el-radio__input.is-checked + .el-radio__label {
    color: #3A6073;
}
.theme3 .el-input.is-active .el-input__inner, .theme3 .el-input__inner:focus {
    border-color: #3A6073;
}
.theme3 .el-button--primary{
    background:#3A6073;
    border-color:#3A6073;
}
.theme3 .el-button--primary:hover{
    background:#3a6073cc;
    border-color:#3a6073cc;
}
.theme3 .el-checkbox__input.is-checked .el-checkbox__inner, 
.theme3 .el-checkbox__input.is-indeterminate .el-checkbox__inner {
    background-color: #3a6073;
    border-color: #3a6073;
}
.theme3 .el-checkbox__inner:hover{
    border-color: #3a6073;
}
.theme3 .el-switch.is-checked .el-switch__core {
    border-color: #3a6073;
    background-color: #3a6073;
}
.theme3 .el-switch__label.is-active {
    color: #3a6073;
}
.theme3 .el-switch__label{
    color: rgba(220,220,220);
}
